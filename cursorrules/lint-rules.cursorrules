{
  "cursorrules": {
    "javascript": {
      "indent": 2,
      "quotes": "single",
      "semi": true,
      "trailingComma": "es5",
      "bracketSpacing": true,
      "jsxBracketSameLine": false,
      "arrowParens": "avoid",
      "printWidth": 80
    },
    "typescript": {
      "extends": "javascript",
      "strictNullChecks": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    },
    "react": {
      "jsxSingleQuote": false,
      "componentStyle": "function",
      "propsInterface": true
    },
    "css": {
      "indentation": 2,
      "sortProperties": true,
      "blockless-after-blockless": true
    }
  }
}